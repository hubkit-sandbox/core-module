{
    "name": "park-manager/core-module",
    "type": "park-manager-module",
    "description": "Park-Manager CoreSystem",
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "ext-sodium": "*",
        "beberlei/assert": "^3.0.0",
        "doctrine/dbal": "^2.6.1",
        "doctrine/doctrine-bundle": "^1.9.2",
        "doctrine/orm": "^2.6.2",
        "egulias/email-validator": "^2.1",
        "paragonie/constant_time_encoding": "^2.0",
        "paragonie/halite": "^4.4.2",
        "ramsey/uuid": "^3.0",
        "rollerworks/route-autofill-bundle": "^0.1.0",
        "rollerworks/route-autowiring-bundle": "^1.0.2",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^4.2",
        "symfony/console": "^4.2",
        "symfony/expression-language": "^4.2",
        "symfony/flex": "^1.1.1",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/messenger": "^4.2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.2",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/translation": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "johnkary/phpunit-speedtrap": "v3.0.0",
        "matthiasnoback/symfony-config-test": "^4.0.0",
        "matthiasnoback/symfony-dependency-injection-test": "^3.0.0",
        "phpunit/phpunit": "^7.2.7",
        "symfony/browser-kit": "^4.2",
        "symfony/debug-pack": "^1.0.5",
        "symfony/dotenv": "^4.2",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/profiler-pack": "^1.0.3",
        "symfony/web-server-bundle": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\Module\\CoreModule\\": ""
        },
        "exclude-from-classmap": [
            "Tests/",
            "Infrastructure/Resources/"
        ]
    },
    "autoload-dev": {
        "ParkManager\\Module\\CoreModule\\Tests\\": "Tests"
    }
}
